{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "globals":{
      "weex": 1
    },
    "rules": {
        "func-names": 0,
        "comma-dangle": 0,
        "object-shorthand": 0,
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "consistent-return": 0,
        "prefer-template": 0
    }
}